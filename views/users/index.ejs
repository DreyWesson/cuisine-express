<div class="container">
  <h2>Users Table</h2>
  <table class="table table-striped table-dark">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Zip Code</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(user => { %>
      <tr>
        <td><%= user.fullName %></td>
        <td><%= user.email %></td>
        <td>@<%= user.zipCode %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
